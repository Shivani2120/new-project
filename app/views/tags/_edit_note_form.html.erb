
  <%= form_for tag, url: update_note_tag_path(@tag), method: :patch, remote: true do |form| %>
  <div class="modal-body">
      <div class="form-group mt-3">
          <h5><%= form.label :title %><h5>
          <%= form.text_field :title, class: "form-control" %>
      </div>
    
      <div>
        <%= form.submit "Update Notes", class: "btn btn-outline-info btn-lg close-modal" %>
        <button type="button" class="btn btn-outline-info btn-lg close-modal" data-bs-dismiss="modal">Close</button>
      </div>
    </div>
  <% end %>



